generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url = env("DATABASE_URL")
}

model User {
    id Int @id @default(autoincrement())
    name String
    email String @unique
    password String
    createdAt DateTime @default(now())
    sessions Session[]
}

model Session {
    id Int @id @default(autoincrement())
    userId Int
    user User @relation(fields: [userId], references: [id])
    token String
}